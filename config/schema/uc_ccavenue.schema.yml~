# Schema for the configuration files of the uc_ccavenue module.

payment_method.settings.ccavenue:
  type: mapping
  label: 'CCAvenue settings'
  mapping:
    ccavenue_working_key:
      type: string
      label: 'Working Key'
    ccavenue_merchant_id:
      type: uri
      label: 'Merchant ID'
    ccavenue_access_code:
      type: string
      label: 'Access Code'
		ccavenue_server:
      type: string
      label: 'CCAvenue Server'
